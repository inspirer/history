#output .com
#org 0x100
jmp start
#include $crt.ch
#include $keyb.ch
#include $system.ch
start:

  writeln("Keyboard SCAN code reader v0.99")
  while(ax!=1){
    xor ax,ax
    delay(100)
    in al,0x60
    and ax,0x7F
    push ax
    getYpos()
    GotoXY(0,ax)
    while(kbhit()!=0){ readkey() }
    pop ax
    write("AX=")
    writehexw(ax)
  }
  writeln(null)

  ret

#data
